      "/Contacts/call-list": {
        "get": {
          "tags": [
            "Contacts"
          ],
          "responses": {
            "default": {
              "description": "Error",
              "content": {
                "text/plain": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/CallRecord"
                    }
                  }
                },
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/CallRecord"
                    }
                  }
                },
                "text/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/CallRecord"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "components": {
      "schemas": {
        "AddressDTO": {
          "type": "object",
          "properties": {
            "street": {
              "type": "string",
              "nullable": true
            },
            "city": {
              "type": "string",
              "nullable": true
            },
            "state": {
              "type": "string",
              "nullable": true
            },
            "zip": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "CallRecord": {
          "type": "object",
          "properties": {
            "name": {
              "$ref": "#/components/schemas/Name"
            },
            "phone": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "ContactDTO": {
          "required": [
            "name"
          ],
          "type": "object",
          "properties": {
            "name": {
              "$ref": "#/components/schemas/NameDTO"
            },
            "address": {
              "$ref": "#/components/schemas/AddressDTO"
            },
            "phone": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/PhoneDTO"
              },
              "nullable": true
            },
            "email": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "ExistingContactDTO": {
          "required": [
            "name"
          ],
          "type": "object",
          "properties": {
            "name": {
              "$ref": "#/components/schemas/NameDTO"
            },
            "address": {
              "$ref": "#/components/schemas/AddressDTO"
            },
            "phone": {
              "type": "array",
              "items": {
                "$ref": "#/components/schemas/PhoneDTO"
              },
              "nullable": true
            },
            "email": {
              "type": "string",
              "nullable": true
            },
            "id": {
              "type": "integer",
              "format": "int32"
            }
          },
          "additionalProperties": false
        },
        "Name": {
          "type": "object",
          "properties": {
            "first": {
              "type": "string",
              "nullable": true
            },
            "middle": {
              "type": "string",
              "nullable": true
            },
            "last": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "NameDTO": {
          "type": "object",
          "properties": {
            "first": {
              "type": "string",
              "nullable": true
            },
            "middle": {
              "type": "string",
              "nullable": true
            },
            "last": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "PhoneDTO": {
          "type": "object",
          "properties": {
            "number": {
              "type": "string",
              "nullable": true
            },
            "type": {
              "$ref": "#/components/schemas/PhoneTypeDTO"
            }
          },
          "additionalProperties": false
        },
        "PhoneTypeDTO": {
          "enum": [
            "Home",
            "Work",
            "Mobile"
          ],
          "type": "string"
        }
      }
    }
  }