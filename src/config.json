{
    "openapi": "3.0.1",
    "servers": [
        {
          "url": "http://localhost:21268",
          "description": "Development server"
        }
    ],
    "info": {
      "title": "Contacts API",
      "description": "Api for CRUD operations against a contact list.",
      "contact": {
        "name": "Jordan Upperman",
        "url": "https://github.com/Jupperman/ContactsApi",
        "email": "Jordaneupperman@gmail.com"
      },
      "version": "v1"
    },
    "paths": {
      "/Contacts": {
        "get": {
          "tags": [
            "Contacts"
          ],
          "responses": {
            "default": {
              "description": "Error",
              "content": {
                "text/plain": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/ExistingContactDTO"
                    }
                  }
                },
                "application/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/ExistingContactDTO"
                    }
                  }
                },
                "text/json": {
                  "schema": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/ExistingContactDTO"
                    }
                  }
                }
              }
            }
          }
        },
        "post": {
          "tags": [
            "Contacts"
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ContactDTO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ContactDTO"
                }
              },
              "application/*+json": {
                "schema": {
                  "$ref": "#/components/schemas/ContactDTO"
                }
              }
            }
          },
          "responses": {
            "default": {
              "description": "Error",
              "content": {
                "text/plain": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                },
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                },
                "text/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                }
              }
            }
          }
        }
      },
      "/Contacts/{id}": {
        "get": {
          "tags": [
            "Contacts"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "default": {
              "description": "Error",
              "content": {
                "text/plain": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                },
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                },
                "text/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                }
              }
            }
          }
        },
        "delete": {
          "tags": [
            "Contacts"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Success"
            }
          }
        },
        "put": {
          "tags": [
            "Contacts"
          ],
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "integer",
                "format": "int32"
              }
            }
          ],
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ContactDTO"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ContactDTO"
                }
              },
              "application/*+json": {
                "schema": {
                  "$ref": "#/components/schemas/ContactDTO"
                }
              }
            }
          },
          "responses": {
            "default": {
              "description": "Error",
              "content": {
                "text/plain": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                },
                "application/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                },
                "text/json": {
                  "schema": {
                    "$ref": "#/components/schemas/ExistingContactDTO"
                  }
                }
              }
            }
          }
        }
